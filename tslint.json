{
  "extends": ["tslint-config-airbnb"],
  "rules": {
    "max-line-length": [true, 120],
    "object-shorthand-properties-first": false,
    "radix": false,
    "ter-arrow-parens": false,
    "import-name": false,
    "variable-name": false,
    "no-consecutive-blank-lines": [ false ],
    "no-unused-expression": [
      true,
      "allow-new"
    ],
    "ordered-imports": [ false ]
  }
}